#!ipxe

cpuid --ext 29 && set arch amd64 || set arch x86
dhcp
set boot-url http://github.gogodoge66.eu.org/https://github.com/veeyoung/somescripts/releases/download/static
imgfree
initrd -n bootmgr     ${boot-url}/bootmgr       bootmgr
initrd -n bootmgr.efi ${boot-url}/bootmgr.efi   bootmgr.efi
initrd -n bcd         ${boot-url}/bcd           bcd
initrd -n boot.sdi    ${boot-url}/boot.sdi      boot.sdi
initrd -n boot.wim    ${boot-url}/boot.wim      boot.wim
boot
