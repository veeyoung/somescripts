#!ipxe

cpuid --ext 29 && set arch amd64 || set arch x86
dhcp
set boot-url http://github.gogodoge66.eu.org/https://github.com/veeyoung/somescripts/releases/download/static
kernel wimboot
initrd ${boot-url}/bootmgr  bootmgr
initrd ${boot-url}/bcd      BCD
initrd ${boot-url}/boot.sdi boot.sdi
initrd ${boot-url}/boot.wim boot.wim
boot
